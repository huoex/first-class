<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="INTEGER" hibernate-type="int"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="100"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="LONGVARBINARY" hibernate-type="binary"
		not-null="false">
	</sql-type>
</type-mapping>
<table-filter match-catalog="imagelibrary" match-name="category"/>
<table-filter match-catalog="imagelibrary" match-name="image"/>
</hibernate-reverse-engineering>