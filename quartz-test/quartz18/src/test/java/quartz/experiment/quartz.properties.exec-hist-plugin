# A simple and minimal Quartz Scheduler configuration
# See http://www.quartz-scheduler.org/docs/configuration

org.quartz.scheduler.skipUpdateCheck = true
org.quartz.threadPool.class = org.quartz.simpl.SimpleThreadPool
org.quartz.threadPool.threadCount = 5

# Configure the ExecutionHistoryPlugin
org.quartz.plugin.executionHistory.class = org.quartz.extra.ExecutionHistoryPlugin
